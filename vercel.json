{
  "version": 2,
  "builds": [
    { "src": "api/yt2mp3.py", "use": "@vercel/python" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/yt2mp3.py" },
    { "src": "/", "dest": "/static/index.html" },
    { "src": "/(.*)", "dest": "/static/$1" }
  ]
}
